{
    "process": {
        "terminal": true,
        "consoleSize": {
            "height": 25,
            "width": 80
        },
        "user": {
            "uid": 1,
            "gid": 1,
            "umask": 63,
            "additionalGids": [5, 6]
        },
        "env": [
            "PATH=/usr/local/sbin:/usr/local/bin:/usr/sbin:/usr/bin:/sbin:/bin",
            "TERM=xterm"
        ],
        "cwd": "/root",
        "args": [
            "/bin/sh",
            "ls"
        ]
    }
}